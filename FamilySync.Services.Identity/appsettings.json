{
  "ConnectionStrings": {
    "MySQL": "Server=127.0.0.1;Port=3306;Database=example;Uid=root;"
  },
  "Config": {
    "Service": {
      "Name": "Identity Service",
      "Description": "Authentication provider responsible for Identities/Users",
      "Route": "api",
      "IsDevelopment": true
    },
    "Inclusion": {
      "MVC": true,
      "Swagger": true,
      "Versioning": true,
      "Mapper": true,
      "Authorization": true
    },
    "Authentication": {
      "Secret": "SuperLongSecretThatShouldNotBePublic",
      "Issuer": "localhost",
      "Audience": "localhost"
    }
  },
  "AuthTokenConfig": {
    "AccessToken": {
      "LifeTimeInMinutes": 5
    },
    "RefreshToken": {
      "CookieKey": "familysync.refresh",
      "LifeTimeInMinutes": 4380
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
