{
  "ConnectionStrings": {
    "MySQL": "Server=127.0.0.1;Port=3306;Database=example;Uid=root;"
  },
  "Config": {
    "Service": {
      "Name": "Identity Service",
      "Description": "Authentication provider responsible for Identities/Users",
      "Route": "api",
      "Debug": true
    },
    "Inclusion": {
      "MVC": true,
      "Swagger": true,
      "Versioning": true,
      "Mapper": true,
      "Authorization": true
    },
    "Authentication": {
      "Secret": "SuperLongSecretThatShouldNotBePublic",
      "Issuer": "localhost",
      "Audience": "localhost"
    },
    "Cors": {
      "Name": "Blazor",
      "Origins": [
        "https://localhost:44392",
        "https://localhost:5176",
        "http://localhost:5176"
      ]
    },
    "TelemetryLogging": {
      "Enabled": true,
      "Endpoint": "http://localhost:5341/ingest/otlp/v1/logs",
      "Key": "5zlOgtgspIxPldE3uxdg"
    }
  },
  "AuthTokenConfig": {
    "AccessToken": {
      "LifeTimeInMinutes": 5
    },
    "RefreshToken": {
      "CookieKey": "familysync.refresh",
      "LifeTimeInMinutes": 4380
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
